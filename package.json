{
  "name": "fvtt-quench",
  "version": "0.5.0",
  "description": "Harden your Foundry module or system code with end-to-end UI tests directly within Foundry. Powered by Mocha.",
  "main": "dist/quench.js",
  "contributors": [
    {
      "name": "Cole Schultz",
      "discord": "cole#9640"
    },
    {
      "name": "Ethaks",
      "discord": "Ethaks#2903"
    }
  ],
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "format": "prettier --write \"./**/*.(js|mjs|ts|json|css|yml)\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "build": "gulp build",
    "build:watch": "gulp watch",
    "clean": "gulp clean",
    "bump-version": "gulp bumpVersion",
    "release": "gulp build && tsc --emitDeclarationOnly false --noEmit && standard-version",
    "compile": "tsc --emitDeclarationOnly false --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ethaks/FVTT-Quench.git"
  },
  "author": "Ethaks",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Ethaks/FVTT-Quench/issues"
  },
  "homepage": "https://github.com/Ethaks/FVTT-Quench#readme",
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@league-of-foundry-developers/foundry-vtt-types": "^0.8.9-7",
    "@sindresorhus/fnv1a": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "esbuild": "^0.13.12",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "pretty-format": "^27.2.5",
    "semver": "^7.3.5",
    "standard-version": "^9.3.2",
    "typescript": "^4.4.4",
    "yargs": "^17.2.1"
  },
  "lint-staged": {
    "*.(ts|js)": "eslint --cache --fix",
    "*.(json|css|yml)": "prettier --write"
  }
}
